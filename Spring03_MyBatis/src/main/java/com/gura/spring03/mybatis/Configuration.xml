<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases> 
		<typeAlias type="com.gura.spring03.member.dto.MemberDto" alias="memberDto"/>
		<typeAlias type="com.gura.spring03.users.dto.UsersDto" alias="usersDto"/>
		<typeAlias type="com.gura.spring03.file.dto.FileDto" alias="fileDto"/>
		<typeAlias type="com.gura.spring03.cafe.dto.CafeDto" alias="cafeDto"/>
